<div class="wrapper">
	<div class="login-form">
	<%= form_tag "/sessions", method: :post do %>
		<p>
	  <%= label_tag :username %>
	  <%= text_field_tag :username %>
		</p>
		<p>
	  <%= label_tag :password %>
	  <%= password_field_tag :password %>
		</p>
	  <%= submit_tag "Login" %>
	<% end %>

	<%= button_to "Create a new Account", new_user_path, method: :get %>
	</div>
</div>
