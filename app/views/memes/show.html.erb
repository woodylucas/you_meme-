<%= image_tag url_for(@meme.cover_photo) %>
 <%= link_to @meme.cover_photo.filename, rails_blob_path(@meme.cover_photo, disposition: "attachment")
<%= form_for @meme do |f| %>
<%= f.hidden_field :user_id, value: current_user.id %>
<%= f.hidden_field :meme_id, value: @meme.id %>
<%= f.submit "Like" %> + <%= @total_likes %>
<% end %>
